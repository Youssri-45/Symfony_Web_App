{% extends ':base_front:register-page.html.twig' %}
{% block errors %}
    {% for error in errors %}
        <div class="alert alert-danger" role="alert">{{ error.message }}</div>
    {% endfor %}
{% endblock %}

{% block register %}
    {{ form_start(form) }}
    <div class="form-group">
        {{ form_row(form.prenom) }}
    </div>
    <div class="form-group">
        {{ form_row(form.nom) }}
    </div>
    <div class="form-group">
        {{ form_row(form.numTel) }}
    </div>
    <div class="form-group">
        {{ form_row(form.email) }}
    </div>
    <div class="form-group">
        {{ form_row(form.password.first) }}
    </div>
    <div class="form-group">
        {{ form_row(form.password.second) }}
    </div>
    <div class="form-group">
        <button type="submit" id="button" name="button" class="btn tp-btn tp-btn-orange">Submit</button>
    </div>
        {{ form_end(form) }}


{% endblock %}